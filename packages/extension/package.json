{
  "name": "proxy-manager-extension",
  "displayName": "Proxy manager extension",
  "version": "1.0.0",
  "description": "Mange your proxies through extension ",
  "author": "Developer",
  "scripts": {
    "start": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^0.13.0",
    "plasmo": "0.59.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shortid": "^2.2.16",
    "tw-elements": "1.0.0-alpha13"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.0",
    "@types/chrome": "0.0.202",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "prettier": "2.8.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*",
      "<all_urls>"
    ],
    "permissions": [
      "tabs",
      "proxy",
      "webRequest",
      "webRequestAuthProvider"
    ]
  }
}